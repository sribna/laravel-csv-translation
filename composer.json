{
  "name": "sribna/laravel-csv-translation",
  "description": "Store Laravel translations in CSV files",
  "keywords": [
    "laravel",
    "laravel-localization"
  ],
  "homepage": "https://github.com/sribna/laravel-csv-translation",
  "license": "MIT",
  "authors": [
    {
      "name": "Iurii Makukh",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Sribna\\CsvTranslation\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sribna\\CsvTranslation\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sribna\\CsvTranslation\\CsvTranslationServiceProvider"
      ]
    }
  }
}
